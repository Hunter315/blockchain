<div className='Block'>\r\n        <div>Hash: {hashDisplay}</div>\r\n        <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\r\n        {this.displayTransaction}\r\n      </div>\r\n    );\r\n  }\r\n};\r\n\r\nexport default Block;"},"lineCount":null}},"error":null,"hash":"88bf4873ee37d26613247d69c4cbaa74","cacheData":{"env":{}}}