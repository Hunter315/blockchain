      .then(json => this.setState({ blocks: json }));\r\n  }\r\n\r\n  render() {\r\n    console.log('this.state', this.state);\r\n\r\n    return (\r\n      <div>\r\n        <div><Link to='/'>Home</Link></div>\r\n        <h3>Blocks</h3>\r\n        <div>\r\n          {\r\n            [...Array(Math.ceil(this.state.blocksLength / 5)).keys()].map(key => {\r\n              const paginatedId = key + 1;\r\n\r\n              return (\r\n                <span key={key} onClick={this.fetchPaginatedBlocks(paginatedId)}>\r\n                  <Button bsSize=\"small\" bsStyle=\"danger\">\r\n                    {paginatedId}\r\n                  </Button>{' '}\r\n                </span>\r\n              )\r\n            })\r\n          }\r\n        </div>\r\n        {/* {\r\n          this.state.blocks.map(block => {\r\n            return (\r\n              <Block key={block.hash} block={block} />\r\n            );\r\n          })\r\n        } */}\r\n{ console.log(this.state.blocks)}\r\n        {\r\n         \r\n          \r\n          <BasicTimeline props={this.state.blocks} />\r\n\r\n        }\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nconst BasicTimeline = props =>  {\r\n\r\n  let blocks = props.props;\r\n  console.log(\"Blocks\", blocks)\r\n\r\nreturn (\r\n  <Timeline>\r\n   \r\n    {blocks.map(block => {\r\n      return (\r\n        <TimelineItem key={block.hash}>\r\n      <TimelineSeparator>\r\n        <TimelineDot />\r\n        <TimelineConnector />\r\n      </TimelineSeparator>\r\n      <TimelineContent>\r\n      <Block block={block} />\r\n        </TimelineContent>\r\n    </TimelineItem>\r\n        \r\n      )\r\n    })}\r\n   </Timeline >\r\n);\r\n  \r\n}\r\n\r\nexport default Blocks;"},"lineCount":null}},"error":null,"hash":"9baaa6eecdad8d2d681ca699a5d7a004","cacheData":{"env":{}}}